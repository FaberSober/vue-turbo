{
  "name": "vue-turbo",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "husky install .husky",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "lint-staged": "lint-staged",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@fa/eslint-config-custom": "workspace:*",
    "@fa/tsconfig": "workspace:*",
    "eslint": "^8.57.0",
    "husky": "8.0.3",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "stylelint": "^16.2.1",
    "turbo": "^1.13.2"
  },
  "packageManager": "pnpm@8.6.10"
}
