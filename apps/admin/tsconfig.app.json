{
  "extends": "@fa/tsconfig/vue.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "./vite.config.ts", "./typed-router.d.ts", "./auto-imports.d.ts"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
    },
    "noImplicitAny": false,
    "allowJs": true,
    "moduleResolution": "Bundler",
    "types": [
      // ...
      "unplugin-vue-router/client"
    ],
  }
}
